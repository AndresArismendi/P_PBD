<h1>Listing enterprise_clients</h1>

<table>
  <thead>
    <tr>
      <th>Run cli</th>
      <th>Rut ent</th>
      <th>Nom cli</th>
      <th>Apells cli</th>
      <th>Tel1 cli</th>
      <th>Tel2 cli</th>
      <th>Email cli</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @enterprise_clients.each do |enterprise_client| %>
      <tr>
        <td><%= enterprise_client.run_cli %></td>
        <td><%= enterprise_client.rut_ent %></td>
        <td><%= enterprise_client.nom_cli %></td>
        <td><%= enterprise_client.apells_cli %></td>
        <td><%= enterprise_client.tel1_cli %></td>
        <td><%= enterprise_client.tel2_cli %></td>
        <td><%= enterprise_client.email_cli %></td>
        <td><%= link_to 'Ver', enterprise_client %></td>
        <td><%= link_to 'Editar', edit_enterprise_client_path(enterprise_client) %></td>
        <td><%= link_to 'Eliminar', enterprise_client, method: :delete, data: { confirm: '¿Confirma eliminación?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo', new_enterprise_client_path %>
