<h1>Listing state_histories</h1>

<table>
  <thead>
    <tr>
      <th>Id trav</th>
      <th>Id stat</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @state_histories.each do |state_history| %>
      <tr>
        <td><%= state_history.id_trav %></td>
        <td><%= state_history.id_stat %></td>
        <td><%= button_to 'Ver', state_history , :method => 'get', :class => 'btn btn-info'%></td>
        <td><%= button_to 'Editar', edit_state_history_path(state_history) , :method => 'get', :class => 'btn btn-warning'%></td>
        <td><%= button_to 'Eliminar', state_history, method: :delete, data: { confirm: '¿Confirma eliminación?' } , :class => 'btn btn-danger'%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= button_to 'Nuevo', new_state_history_path, :method => 'get', :class => 'btn btn-default'%>
