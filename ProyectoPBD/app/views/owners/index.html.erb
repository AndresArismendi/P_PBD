<h1>Listing owners</h1>

<%@owners.each do |n| %>

<ul>
      <li><strong>Nombre: <%=n.nom_prop%></strong><br></li>
      <li><strong>Apellidos: <%=n.apells_prop%></strong><br></li>
      <li><strong>Telefono 1: <%=n.tel1_prop%></strong><br></li>
      <li><strong>Telefono 2: <%=n.tel2_prop%></strong><br></li>
      <li><strong>Email: <%=n.email_prop%></strong><br></li>
      



</ul>     
<%end%>





<table>
  <thead>
    <tr>
      <th>Run own</th>
      <th>Nom prop</th>
      <th>Apells prop</th>
      <th>Tel1 prop</th>
      <th>Tel2 prop</th>
      <th>Email prop</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @owners.each do |owner| %>
      <tr>
        <td><%= owner.run_own %></td>
        <td><%= owner.nom_prop %></td>
        <td><%= owner.apells_prop %></td>
        <td><%= owner.tel1_prop %></td>
        <td><%= owner.tel2_prop %></td>
        <td><%= owner.email_prop %></td>
        <td><%= link_to 'Ver', owner %></td>
        <td><%= link_to 'Editar', edit_owner_path(owner) %></td>
        <td><%= link_to 'Eliminar', owner, method: :delete, data: { confirm: '¿Confirma eliminación?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo', new_owner_path %>
